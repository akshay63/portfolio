{
  "name": "portfolio-ver.1.0",
  "version": "1.0.0",
  "description": "My First portfolio",
  "main": "index.js",
  "browserslist": "last 10 versions",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/main.css -w",
    "compile:sass": "node-sass sass/main.scss css/main.comp.css",
    "prefix:css": "postcss css/main.comp.css --use autoprefixer -o css/main.prefix.css",
    "compress:css": "node-sass css/main.prefix.css css/main.css --output-style compressed",
    "build:css": "compile:sass prefix:css compress:css"
  },
  "author": "Akshay Meshram",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.12",
    "postcss-cli": "^8.3.1",
    "postcss-scss": "^3.0.5"
  }
}
